	},
	"item": [
		{
			"name": "Consultar Llave codigos 400",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 400 or 404\", function () {\r",
							"    pm.expect(pm.response.code).to.be.oneOf([400, 404]);\r",
							"});\r",
							"\r",
							"// Captura la variable code\r",
							"const responseJson = pm.response.json();\r",
							"const code = responseJson.errors[0].code;\r",
							"const codenum = Number(code);\r",
							"pm.environment.set(\"codenum\", codenum);\r",
							"\r",
							"// Verifica que el c√≥digo de estado sea (400 o 404) o que el codenum sea 1031\r",
							"pm.test(\"Status code is (400 or 404) or codenum is 1031\", function () {\r",
							"    const statusCode = pm.response.code;\r",
							"    pm.expect(\r",
							"        (statusCode === 400 || statusCode === 404) || codenum === 1031\r",
							"    ).to.be.true;\r",
							"});\r",
							"\r",
							"// Imprimir el comentario en la consola de Newman\r",
							"console.log(\"Comentario del caso de prueba: \", pm.iterationData.get(\"comentario\"));"
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
